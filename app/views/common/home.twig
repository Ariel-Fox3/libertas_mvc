<div class="container-fluid container-forms" id="form-liberte" style="background-image: url(assets/img/bg-forms.png);">
  <div class="container">
    <div class="row" style="min-height: 500px;">
      <div class="col-12 col-md-8 col-lg-6 mx-auto my-auto">
        <form method="post">
          <div class="form-group">
            <input type="text" class="form-control" id="inputNome" name="nome" placeholder="Nome">
          </div>
          <div class="form-group">
            <input type="email" class="form-control" id="inputEmail" name="email" placeholder="E-mail">
          </div>
          <div class="form-group">
            <input type="text" class="form-control" id="inputTelefone" name="telefone" placeholder="Telefone">
          </div>
          <div class="form-group">
            <textarea id="inputMotivos" name="motivos" class="form-control" rows="4" placeholder="O que você precisa para ser libertado?"></textarea>
          </div>
          <div class="row">
            <div class="col-12 d-flex align-items-center justify-content-center py-4">
              <div id="g_recapcha_div"></div>
            </div>
          </div>
          <div class="row">
            <div class="col-8 col-md-4 mx-auto">
              <button type="submit" class="btn btn-enviar">Enviar</button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>

